<HBox prefHeight="700.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.uniquindio.bases.supermarket.SuperMarketCampestre.controller.LogginViewController">
   <children>
      <Pane prefHeight="400.0" prefWidth="53.0" HBox.hgrow="ALWAYS">
         <children>
            <AnchorPane layoutX="39.0" layoutY="50.0" opacity="0.5" prefHeight="286.0" prefWidth="220.0" style="-fx-background-radius: 20px; -fx-background-color: #628f5b;">
               <children>
                  <TextField fx:id="userField" layoutX="12.0" layoutY="105.0" prefHeight="16.0" prefWidth="191.0" promptText="Ingresa tu Usuario" style="-fx-background-radius: 20px;" />
