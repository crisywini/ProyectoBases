                  <Button layoutX="116.0" layoutY="239.0" mnemonicParsing="false" onAction="#handleLogginButton" style="-fx-background-radius: 20px;" text="Ingresar" />
                  <Label layoutX="32.0" layoutY="79.0" text="Usuario" />
                  <Label layoutX="32.0" layoutY="159.0" text="Contraseña" />
                  <Label layoutX="69.0" layoutY="29.0" text="Ingresar">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
         </children></Pane>
      <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <children>
            <ImageView fx:id="image" fitHeight="198.0" fitWidth="399.0" layoutX="85.0" layoutY="488.0" pickOnBounds="true" preserveRatio="true" />
         </children></Pane>
   </children>
</HBox>
