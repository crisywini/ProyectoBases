<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="menuPane" prefHeight="800.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="co.uniquindio.bases.supermarket.SuperMarketCampestre.controller.AuxiliarMenuViewController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
            <Menu mnemonicParsing="false" text="&lt;">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleBackMenu" text="Volver al inicio" />
               </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Ventas">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleVentasMenu" text="Realizar venta" />
               </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Productos">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleProductsMenu" text="Ver detalle productos" />
               </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
